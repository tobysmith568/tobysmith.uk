---
import { getCollection } from "astro:content";
import ProseLayout from "../layouts/ProseLayout.astro";

const policies = await getCollection("policies");
const termPolicy = policies.find(p => p.slug === "licenses");

if (!termPolicy) {
  throw new Error("Cannot find the third-party licenses policy");
}

const { Content } = await termPolicy.render();
---

<ProseLayout title="Third-Party Licenses">
  <Content />
</ProseLayout>
